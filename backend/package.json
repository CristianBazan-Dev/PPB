{
  "name": "planeta-precios-bajos-e-commerce",
  "version": "1.0.0",
  "description": "an e-commerce for a technology store",
  "main": "server.js",
  "homepage": "https://www.planetapreciosbajos.com/", 
  "scripts": {
    "start": "nodemon server.js",
    "server": "nodemon server.js",
    "client": "cd client && npm run start",
    "server-install": "npm install",
    "client-install": "cd client && npm install",
    "install-all": "concurrently \"npm run server-install\" \"npm run client-install\"",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "cd ../frontend && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CristianBazan-dev/planeta-precios-bajos-e-commerce.git"
  },
